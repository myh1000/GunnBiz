<html lang="en"><head>

	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="">
	<meta name="author" content="">
	<link rel="icon" type="image/png" href="img/favicon.png">

	<title><%= title %></title>

	<!-- Bootstrap Core CSS -->
	<link href="/css/bootstrap.min.css" rel="stylesheet">

	<!-- Custom CSS -->
	<link href="/css/style.css" rel="stylesheet">

	<!-- Custom Fonts -->
	<link href="/css/font-awesome.min.css" rel="stylesheet" type="text/css">
	<link href="/css/myriad.css" rel="stylesheet" type="text/css">

	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
		<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>

<body>

	<!-- Navigation -->
	<%- include('partials/navbar') %>

	<div class="container">
		<div style="padding: 50px 0 20px 0">
			<h1 style="font-size: 35px;">User Info: <%= user.firstName %> <%= user.lastName %> </h1>
		</div>
	</div>
	<div class="container">
		<div class="row">
			<!-- <li>Email: <%= user.email %></li>
			<li>Year: <%= user.year %></li>
			<li>Student ID: <%= user.studentID %></li> -->
		</div>
		<div class="col-lg-12">
            <ul id="myTab" style="margin-bottom: 20px;" class="nav nav-tabs nav-justified">
				<li class="active"><a href="#service-one" data-toggle="tab" aria-expanded="true"><i class="fa fa-briefcase"></i> Status</a></li>
				<li class=""><a href="#service-four" data-toggle="tab" aria-expanded="false"><i class="fa fa-user"></i> Profile</a></li>
				<li class=""><a href="/admin" ><i class="fa fa-lock"></i> Admin Panel</a></li>
            </ul>

            <div id="myTabContent" class="tab-content">
				<div class="tab-pane fade active in" id="service-one">
					<div class="col-md-12">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 style="font-size: 20px;"><i class="fa fa-briefcase"></i> Status</h4>
							</div>
							<div class="panel-body">
								<h1 style="font-size: 20px; line-height:5px; color: grey">DECA Events:</h1>
								<h1 style="font-size: 20px; line-height:5px; color: grey">FBLA Events:</h1>
								<h1 style="font-size: 20px; line-height:5px; color: grey">Teams:</h1>
								<h1 style="font-size: 20px; line-height:5px; color: grey">Conferences:</h1>
								<h1 style="font-size: 20px; line-height:5px; color: grey">Checks:<% if (user.FBLA_regionalsForms == null){
									%>
									<font style="color:red;">Not Submitted</font>
									<%
								}	else{
									%>
									<font style="color:green">Submitted</font>
									<%
								}
								%></h1>
								<h1 style="font-size: 20px; line-height:5px; color: grey">Forms:<% if (user.FBLA_regionalsChecks == null){
									%>
									<font style="color:red;">Not Submitted</font>
									<%
								}	else{
									%>
									<font style="color:green">Submitted</font>
									<%
								}
								%></h1>
							</div>
						</div>
					</div>
				</div>
				<div class="tab-pane fade" id=	"service-two">
					<div class="col-md-12">
			            <div class="panel panel-default">
			                <div class="panel-heading">
								<h4 style="font-size: 20px;"><i class="fa fa-briefcase"></i> DECA Materials</h4>
			                </div>
			                <div class="panel-body">
								<h2 style="font-size: 20px; line-height:5px; color: grey">Coming soon.</h2>
			                </div>
			            </div>
			        </div>
				</div>
                <div class="tab-pane fade" id="service-three">
					<div class="col-md-12">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 style="font-size: 20px;"><i class="fa fa-briefcase"></i> FBLA Materials</h4>
							</div>
							<div class="panel-body">
								<h2><a style="font-size: 18px; line-height:0px" href="https://drive.google.com/drive/u/0/folders/0B7x5K6sEPxSpYXA5UXQ2TmswSms"> Practice Materials </a></h2>
							</div>
						</div>
					</div>
				</div>
				<div class="tab-pane fade" id="service-four">
					<div class="col-md-12">
			            <div class="panel panel-default">
			                <div class="panel-heading">
								<h4 style="font-size: 20px"><i class="fa fa-user"></i> Profile Information</h4>
			                </div>
			                <div class="panel-body">
								<form method="post">
									<div class="form-group">

									    <label>First Name</label>
									    <input type="text" class="form-control" name="firstName" value="<%= user.firstName %>">
									    <label>Last Name</label>
									    <input type="text" class="form-control" name="lastName" value="<%= user.lastName %>">
										<label>Email</label>
									    <input type="text" class="form-control" name="email" value="<%= user.email %>">
									    <label>Grade</label>
									    <select name="grade" class="form-control" id="grade" selected=<%= user.year %>>
									        <%
									            var options = ["Freshman", "Sophomore", "Junior", "Senior"]
									            for(var i = 0; i < options.length; i++) { %>
									                <option <% if (user.year == options[i]) { %>selected<% } %> >  <%= options[i] %>  </option>
									            <% }
									        %>
									    </select>
										<label for="phoneNumber">Phone Number</label>
										<input type="tel" class="form-control" name="phoneNumber" value="<%= user.phoneNumber %>">
										<label>Birthday</label>
									    <input type="date" class="form-control" name="birthday" value="<%= user.birthday %>">
									    <label>Student ID</label>
									    <input type="text" class="form-control" name="studentID" value="<%= user.studentID %>">
									    <label>Parent/Guardian 1 Email</label>
									    <input type="text" class="form-control" name="parent1email" value="<%= user.parent1email %>">
									    <label>Parent/Guardian 2 Email</label>
									    <input type="text" class="form-control" name="parent2email" value="<%= user.parent2email %>">
									</div>
								<button type="submit" name="submitbutton" class="btn btn-wide">Update</button>
								</form>
			                </div>
			            </div>
			        </div>
				</div>
            </div>
        </div>
	</div>

	<div class="modal fade" id="DECAcancel" tabindex="-1" role="dialog" aria-labelledby="contactLabel" aria-hidden="true">
	    <div style="margin: 200px auto;" class="modal-dialog">
	        <div class="modal-content">
	            <div class="modal-header">
	                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
	                <h3 class="modal-title" id="contactLabel">Are you sure?</h3>
	            </div>
	            <div class="modal-body">
					Are you sure you want to cancel signups?
	            </div>
	            <div class="confirm-modal-footer">
					<label style="margin-top: 0px; padding-top: 16px;" for="deca-submit-form" role="button" class="btn btn-default" tabindex="0">YES</label>
					<button type="button" class="btn btn-default" data-dismiss="modal">NO</button>
	            </div>
	        </div>
	    </div>
	</div>

	<div class="modal fade" id="FBLAcancel" tabindex="-1" role="dialog" aria-labelledby="contactLabel" aria-hidden="true">
	    <div style="margin: 200px auto;" class="modal-dialog">
	        <div class="modal-content">
	            <div class="modal-header">
	                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
	                <h3 class="modal-title" id="contactLabel">Are you sure?</h3>
	            </div>
	            <div class="modal-body">
	                Are you sure you want to cancel signups?
	            </div>
	            <div class="confirm-modal-footer">
					<label style="margin-top: 0px; padding-top: 16px;" for="fbla-submit-form" role="button" class="btn btn-default" tabindex="0">YES</label>
					<button type="button" class="btn btn-default" data-dismiss="modal">NO</button>
	            </div>
	        </div>
	    </div>
	</div>

	<!-- Footer -->
	<%- include('partials/footer') %>
	<%- include('partials/contact') %>

	<!-- jQuery -->
	<script src='/javascripts/lib/jquery.min.js'></script>

	<!-- Bootstrap Core JavaScript -->
	<script src="/javascripts/bootstrap.min.js"></script>

	<!-- Custom JavaScript -->

	<!-- <script src="res/js/nav.js"></script> -->




</body></html>
